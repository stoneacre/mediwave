---
import { getEntry, render } from "astro:content";
import Layout from "../layout/Layout.astro";
import BreadCumb from "../components/ui/BreadCumb.astro";
const TermsConditionBody = await getEntry("termsCondition", "index");
let Content;
if (TermsConditionBody) {
  ({ Content } = await render(TermsConditionBody));
}
---

<Layout title='Terms & Condition'>
  <main class='main-wrapper relative overflow-hidden'>
    <BreadCumb>
      <h1 class='breadcrumb-title'>Terms And Condition</h1>
      <ul class='breadcrumb-nav'>
        <li><a href='/'>Home</a></li>
        <li>Terms And Condition</li>
      </ul>
    </BreadCumb>
    <div style="margin: 20px;">
      {Content && <Content />}
    </div>
  </main>
</Layout>
